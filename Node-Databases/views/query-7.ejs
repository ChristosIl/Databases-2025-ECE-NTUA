            <% query7.forEach(row => { %>
                <tr>
                    <td><%= row.FESTIVAL %></td>
                    <td><%= row.AVG_working_experience_of_technicians %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
    
    <br>
    <a href="/">Back to Home</a>
